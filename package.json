{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node -- ./src/bin/server.js",
    "build": "parcel build --detailed-report --target node ./src/bin/server.js",
    "build:staging": "ENV NODE_ENV=staging parcel build --target node ./src/bin/server.js",
    "docker:build": "docker build -t dunghv/getme-api .",
    "docker:build:staging": "docker build -t dunghv/getme-api -f Dockerfile.staging .",
    "docker:run": "docker run -p 6000:5000 -d dunghv/getme-api",
    "clean": "rm -r .cache dist"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.7",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "vcards-js": "^2.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4"
  }
}
